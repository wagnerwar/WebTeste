@using TesteWeb.Controllers;
@{
    ViewBag.Title = "Home Page";
    var urlGerarExcel = Url.Action("GerarExcel", "Home");
    var urlGerarPDF = Url.Action("GerarPDF", "Home");
    var urlImprimir = Url.Action("Imprimir", "Home");
    var urlImprimirPDF = Url.Action("ImprimirPDF", "Home");
}
<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>
<div class="row">
    <h3>Tooltip</h3>
    <a class="btn tooltipped" data-position="bottom" data-tooltip="I am a tooltip">Clique aqui!</a>
</div>
<div class="row">
    <table class="table">
        <thead class="">
            <tr>
                <th>testa</th>
                <th>Teste</th>
            </tr>

        </thead>
        <tbody>
            <tr>
                <th>Dia</th>
                <th>Dia da Semana</th>
            </tr>
        </tbody>
    </table>
</div>
<div class="row">
    <button id="btnGerarExcel" class="btn btn-default">Gerar Excel</button>
    <button id="btnGerarPDF" class="btn btn-default">Gerar PDF</button>
</div>

<!-- Formulário de teste -->
<button id="btnFormulario" class="btn btn-default" data-formulario-arquivo-valido="true" name="btnFormulario" type="button">Enviar formulário</button>

<!-- dropzone -->
@*@Html.Partial("_Arquivo")
    *@
<!-- printThis -->
<div class="opcoes row">
    <button id="btnPrintar" class="btn btn-default" name="btnPrintar" type="button">Imprimir</button>
</div>
<div id="containerPrint">

    <div class="row">
        <h1 id="cabecalhoPrint">Teste</h1>
    </div>
    <div class="row">
        <p>Lorem ipsum ...</p>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        $('.tooltipped').tooltip();
        $("#btnGerarExcel").click(function (ev) {
            ev.preventDefault();
            console.log("Clique no botão de gerar Excel");
            $.ajax({
                url: "@urlGerarExcel",
                success: function (data) {
                    console.log(data);
                    if (data.ok) {
                        window.open("@urlImprimir", "Nova janela");
                    }
                }, error: function (xhr, status, er) {
                    console.log(er);
                }
            });
        });
        $("#btnGerarPDF").click(function (ev) {
            ev.preventDefault();
            console.log("Clique no botão de gerar Excel");
            $.ajax({
                url: "@urlGerarPDF",
                success: function (data) {
                    console.log(data);
                    if (data.ok) {
                        window.open("@urlImprimirPDF", "Nova janela");
                    }
                }, error: function (xhr, status, er) {
                    console.log(er);
                }
            });
        });
        $("#btnFormulario").click(function (ev) {
            ev.preventDefault();
            if (typeof formularioArquivoValido !== 'undefined') {
                if (formularioArquivoValido == false) {
                    console.log("Evento não executado por ter pendência de validação");
                    return false;
                }
            }
            console.log("Executada do formulário");
        });
        $("#btnPrintar").click(function (ev) {
            ev.preventDefault();
            console.log("Printar");
            $("#containerPrint").printThis({
                importCSS: true,
                importStyle: true,
                loadCSS: "@Url.Content("~/Content/print.css")"
            });
        });
    });
</script>